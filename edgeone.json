{
  "rewrites": [
    {
      "source": "/ip", 
      "destination": "https://myip.ipip.net"
    },
    {
      "source": "/ip-rss", 
      "destination": "https://ip.rss.ink"
    },
    {
      "source": "/file-:path*",
      "destination": "https://files.oaiusercontent.com/file-:path*"
    },
    {
      "source": "/:path*", 
      "destination": "https://chatgpt.com/:path*"
    }
  ]
}
